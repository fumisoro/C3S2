{
    "name": "Test1",
    "version": "0.1",
    "manifest_version": 2,
    
    "description": "拡張機能のテスト1",
    
    "permissions": [
        "tabs", "http://*/*"
    ],
    
    "browser_action": {
        "default_icon": "icon48.png",
        "default_title": "Test1",
        "default_popup": "popup.html"
    },

    "content_scripts":[
    {
        "matches": [
        "<all_urls>"
        ],
        
        "js": ["jquery-2.1.4.min.js","content.js"],

        "run_at": "document_start"
    }
    ]
}